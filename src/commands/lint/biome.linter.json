{
	"overrides": [
		{
			"includes": ["**/constants.ts"],
			"linter": {
				"rules": {
					"style": {
						"useFilenamingConvention": {
							"level": "error",
							"options": {
								"filenameCases": ["kebab-case"],
								"requireAscii": true
							}
						}
					}
				}
			}
		}
	],
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"style": {
				"useFilenamingConvention": {
					"level": "error",
					"options": {
						"filenameCases": ["export"],
						"requireAscii": true
					}
				},
				"useConsistentTypeDefinitions": {
					"level": "error",
					"options": {
						"style": "type"
					}
				},
				"useConsistentMemberAccessibility": {
					"level": "error",
					"options": {
						"accessibility": "noPublic"
					}
				}
			}
		}
	}
}
